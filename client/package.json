{
    "name": "sprotty",
    "version": "0.1.0",
    "description": "A next-gen graphics framework",
    "repository": {
        "type": "git",
        "url": "https://github.com/TypeFox/sprotty"
    },
    "main": "index.js",
    "scripts": {
        "prepublish": "typings install --overwrite --save",
        "build": "webpack --progress",
        "build:ui_test": "webpack --config ./test/webpack-test.config.js --progress",
        "build:test-app": "npm run build:ui_test && ./node_modules/.bin/tsc -p test/tsconfig.json",
        "test-app": "node test/test-app.js",
        "watch": "webpack --progress --watch",
        "test": "mocha --opts ./mocha.opts",
        "test:ui": "npm run build:test-app && ./node_modules/.bin/wdio wdio.conf.js"
    },
    "keywords": [],
    "author": "TypeFox",
    "license": "ISC",
    "devDependencies": {
        "@types/chai": "^3.4.35",
        "@types/express": "^4.0.35",
        "@types/mocha": "^2.2.40",
        "@types/node": "^7.0.10",
        "@types/webdriverio": "^4.7.0",
        "chai": "^3.5.0",
        "circular-dependency-plugin": "^2.0.0",
        "express": "^4.15.2",
        "jsdom": "9.12.0",
        "jsdom-global": "2.1.1",
        "mocha": "^3.2.0",
        "reconnecting-websocket": "^3.0.3",
        "reflect-metadata": "^0.1.10",
        "snabbdom-jsx": "^0.3.1",
        "snabbdom-to-html": "^3.1.1",
        "ts-loader": "^2.0.0",
        "ts-node": "^2.1.0",
        "typescript": "^2.2.2",
        "typings": "^2.1.0",
        "wdio-mocha-framework": "^0.5.10",
        "wdio-selenium-standalone-service": "0.0.8",
        "wdio-spec-reporter": "^0.1.0",
        "webdriverio": "^4.8.0",
        "webpack": "^2.2.1"
    },
    "dependencies": {
        "inversify": "^3.1.0",
        "inversify-logger-middleware": "^3.0.0",
        "snabbdom": "^0.6.4"
    }
}